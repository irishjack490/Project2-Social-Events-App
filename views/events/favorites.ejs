<%- include('../partials/header') %>

<div class="row row-cols-1 row-cols-md-2 g-4">
  <% likedEvents.forEach(function(event) { %>
    <div class="col">
      <div class="card custom-card">

        <% if (event.performers && event.performers.length > 0) { %>
            <!-- Display the image using the image URL from the API -->
            <img src="<%= events.performers[2] %>" class="card-img-top" alt="Event Image">
          <% } else { %>
            <!-- Display a default image or handle the case where no image is available -->
            <img src="default-image-url" class="card-img-top" alt="Default Image">
          <% } %>
          
          <div class="card-body">
            <h5 class="card-title"><%= event.type %></h5>
            
          <!-- Add other details you want to display -->
    
        </div>
      </div>
    </div>
  <% }) %>
</div>

<%- include('../partials/footer') %>